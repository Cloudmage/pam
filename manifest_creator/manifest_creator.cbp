<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="manifest_creator" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Windows Debug">
				<Option platforms="Windows;" />
				<Option output="bin/Debug/manifest_creator" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="0" />
				<Option compiler="new_gnu_gcc_compiler" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-DWIN32" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DUNICODE" />
					<Add option="-D__WINDOWS__" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DWXDEBUG" />
					<Add directory="$(#wx3_0.include)" />
					<Add directory="$(#wx3_0.include)/mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx3_0.include)/mswu" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="wxmsw30u_gcc_wammacs" />
					<Add directory="$(#wx3_0.lib)" />
				</Linker>
			</Target>
			<Target title="Windows Release">
				<Option platforms="Windows;" />
				<Option output="bin/Release/manifest_creator" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="0" />
				<Option compiler="new_gnu_gcc_compiler" />
				<Option projectLinkerOptionsRelation="2" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-std=c++11" />
					<Add option="-DWIN32" />
					<Add option="-D__WXMSW__" />
					<Add option="-DWXUSINGDLL" />
					<Add option="-DUNICODE" />
					<Add option="-D__WINDOWS__" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-DWXDEBUG" />
					<Add directory="$(#wx3_0.include)" />
					<Add directory="$(#wx3_0.include)/mswu" />
				</Compiler>
				<ResourceCompiler>
					<Add directory="$(#wx3_0.include)/mswu" />
				</ResourceCompiler>
				<Linker>
					<Add option="-s" />
					<Add library="wxmsw30u_gcc_wammacs" />
					<Add directory="$(#wx3_0.lib)" />
				</Linker>
			</Target>
			<Target title="Linux Release">
				<Option output="bin/LinuxRelease/manifest_creator" prefix_auto="1" extension_auto="1" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="`wx-config --cxxflags`" />
					<Add option="-fpermissive" />
					<Add option="-O3" />
					<Add option="-DwxUSE_UNICODE" />
					<Add option="-D__WXGNU__" />
				</Compiler>
				<Linker>
					<Add option="`wx-config --libs`" />
				</Linker>
			</Target>
		</Build>
		<Unit filename="manifest_creatorApp.cpp" />
		<Unit filename="manifest_creatorApp.h" />
		<Unit filename="version.h" />
		<Extensions>
			<code_completion />
			<debugger />
			<envvars />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
