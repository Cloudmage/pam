		<object class="wmKeyboard" name="ID_M_PKBD1" variable="m_pKbd1" member="yes">
			<pos>0,200</pos>
			<size>600,260</size>
			<fg>#FFFFFF</fg>
			<font>
				<size>12</size>
				<style>normal</style>
				<weight>normal</weight>
				<underlined>0</underlined>
				<family>swiss</family>
				<face>Arial</face>
			</font>
		</object>
