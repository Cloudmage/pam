			<label>Clear</label>
			<pos>500,50</pos>
			<size>80,40</size>
			<handler function="OnbtnClearClick" entry="EVT_BUTTON" />
		</object>
		<object class="wxTimer" name="ID_TIMER1" variable="m_timerSecond" member="yes">
			<interval>1000</interval>
			<handler function="OntimerSecondTrigger" entry="EVT_TIMER" />
		</object>
