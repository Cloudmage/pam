			<size>100,80</size>
			<bg>#800000</bg>
			<handler function="OnbtnRecordClick" entry="EVT_BUTTON" />
		</object>
		<object class="wmLabel" name="ID_M_PLBL2" variable="m_pLbl2" member="yes">
			<label>File Length</label>
			<pos>240,100</pos>
			<size>150,30</size>
			<fg>#FFFFFF</fg>
			<bg>#000000</bg>
		</object>
		<object class="wmLabel" name="ID_M_PLBL3" variable="m_plblTime" member="yes">
			<label>00:00:00</label>
			<pos>390,100</pos>
			<size>150,30</size>
			<fg>#00FF80</fg>
			<bg>#000000</bg>
		</object>
		<object class="wmEdit" name="ID_M_PEDT1" variable="m_pedtFile" member="yes">
			<validation>1280335693</validation>
			<border>uiRect::BORDER_DOWN</border>
			<border_focused>uiRect::BORDER_DOWN</border_focused>
			<background_focused>#B0FFD8</background_focused>
			<foreground_focused>#000000</foreground_focused>
